{
  "name": "undercover_server",
  "version": "1.0.0",
  "description": "Server for undercover game",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc -p .",
    "start": "NODE_ENV=production node lib/index.js",
    "start:dev": "npx ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "npx jest src",
    "lint": "npx eslint -c .eslintrc.js --ext .ts src/**",
    "lint:fix": "npx eslint --fix -c .eslintrc.js --ext .ts src/**"
  },
  "keywords": [
    "websocket",
    "nodejs",
    "typescript"
  ],
  "author": "Christophe Ha",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "typescript": "^4.4.4",
    "typescript-logging": "^0.6.4",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^27.0.2",
    "@types/node": "^14.14.9",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-plugin-import": "^2.25.2",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^1.1.8"
  }
}
